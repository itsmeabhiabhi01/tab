                        <button class="dock-button" onclick="switchDevice('phone')"><i class="fas fa-mobile-alt"></i></button>
                        <button class="dock-button" onclick="switchDevice('tablet')"><i class="fas fa-tablet-alt"></i></button>
                        <button class="dock-button" onclick="switchDevice('pc')"><i class="fas fa-desktop"></i></button>
                        <button class="dock-button" onclick="rotateDevice()"><i class="fas fa-sync-alt"></i></button>
                        <button class="dock-button" onclick="goHome()"><i class="fas fa-arrow-left"></i></button>
